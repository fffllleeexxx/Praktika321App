<Window x:Class="Praktika321App.AddTeacherWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Praktika321App"
        mc:Ignorable="d"
        Title="AddTeacherWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical" Margin="20">
            <TextBlock Text="Tab Number"/>
            <TextBox x:Name="TabNumberTextBox"/>
            <TextBlock Text="Rank"/>
            <TextBox x:Name="RankTextBox"/>
            <TextBlock Text="Degree"/>
            <TextBox x:Name="DegreeTextBox"/>
            <Button Content="Add" Click="AddButton_Click" Margin="0,10,0,0"/>
        </StackPanel>
    </Grid>
</Window>
